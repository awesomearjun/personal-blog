{
  "name": "personal-blog",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "parse": "ts-node --project tsconfig.backend.json backend/parser.mts",
    "start": "ng serve",
    "prebuild": "npm run parse",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.6.2",
  "dependencies": {
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "@vercel/speed-insights": "^1.3.1",
    "@vercel/analytics": "^1.6.1",
    "date-fns": "^4.1.0",
    "rxjs": "~7.8.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.4",
    "@angular/cli": "^21.0.4",
    "@angular/compiler-cli": "^21.0.0",
    "@types/jest": "^30.0.0",
    "@types/minimist": "^1.2.5",
    "gray-matter": "^4.0.3",
    "jsdom": "^27.1.0",
    "marked": "^17.0.1",
    "minimist": "^1.2.8",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
}
